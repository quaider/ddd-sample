<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                        http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
                 version="1.0">
    <entity class="vip.kratos.ddd.zmall.shared.domain.StoredEvent">
        <table name="t_event_store"/>
        <attributes>
            <id name="eventId">
            </id>
            <basic name="eventBody" optional="false">
                <column column-definition="json"/>
            </basic>
            <basic name="occurredOn" optional="false">
            </basic>
            <basic name="typeName" optional="false">
            </basic>
            <basic name="status" optional="false"/>
        </attributes>
    </entity>
</entity-mappings>